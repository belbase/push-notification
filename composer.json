{
    "name": "belbase/push-notification",
    "description": "This is the The Library of Push Notification for Firebase.",
    "type": "library",
    "version": "0.1-alpha",
    "license": "MIT",
    "keywords": [
        "Firebase"
    ],
    "minimum-stability": "alpha",
    "authors": [
        {
            "name": "Deepak Belbase",
            "email": "alpha3@madsstudio.co.in"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support":"^5.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Belbase\\PushNotification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Belbase\\PushNotification\\Providers\\PushNotificationServiceProvider"
            ],
            "aliases": {
                "PushNotification": "Belbase\\PushNotification\\Facades\\PushNotification"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
